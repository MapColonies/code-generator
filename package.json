{
  "name": "@map-colonies/code-generator",
  "version": "1.7.1",
  "description": "This is template for map colonies typescript packages",
  "main": "./dist/src/index.js",
  "scripts": {
    "test:unit": "jest --config=./tests/configurations/unit/jest.config.js",
    "test:integration": "jest --config=./tests/configurations/integration/jest.config.js",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prelint:fix": "npm run format:fix",
    "prelint": "npm run format",
    "lint": "eslint ./src/index.ts",
    "lint:fix": "eslint --fix .",
    "release": "standard-version",
    "test": "npm run test:unit && npm run test:integration",
    "prebuild": "npm run clean",
    "build": "tsc --project tsconfig.build.json",
    "start": "npm run build && node ./dist/src/index.js",
    "start:graphQl": "npm run build && node ./dist/src/index.js -tf=./src/AUTOGENERATED/GraphQLClass.ts -p=raster -gt=graphQl",
    "start:orm": "npm run build && node ./dist/src/index.js -tf=./src/AUTOGENERATED/ORMClass.ts -p=polygonParts -gt=orm -ormd='index, check' -uns='check'",
    "clean": "rimraf dist && rimraf src/AUTOGENERATED",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MapColonies/code-generator.git"
  },
  "author": "MapColonies",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MapColonies/code-generator/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "homepage": "https://github.com/MapColonies/code-generator#readme",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@map-colonies/eslint-config": "^2.2.0",
    "@map-colonies/mc-model-types": "^17.10.3",
    "@map-colonies/prettier-config": "0.0.1",
    "@types/argparse": "^2.0.5",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.12",
    "commitlint": "^11.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.8.1",
    "husky": "^4.3.5",
    "jest": "^26.6.3",
    "jest-create-mock-instance": "^1.1.0",
    "jest-html-reporters": "^2.1.2",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "argparse": "^2.0.1",
    "change-case-all": "^2.1.0",
    "ts-morph": "^10.0.2"
  },
  "peerDependencies": {
    "@map-colonies/mc-model-types": ">=17.10.2"
  },
  "bin": {
    "code-generator": "./dist/src/index.js"
  }
}
